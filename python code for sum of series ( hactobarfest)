def calculate_series_sum(N):
    series_sum = 0.0  # Initialize the sum
    for i in range(1, N + 1):
        series_sum += 1.0 / i
    
    return series_sum
N = 10  
if N > 0:
    sum_of_series = calculate_series_sum(N)
    print(f"The sum of the series for N = {N} is: {sum_of_series}")
else:
    print("N should be a positive integer.")
